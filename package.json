{
  "name": "wiredcraft-test",
  "version": "0.0.1",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "jest --verbose",
    "watch": "jest --verbose --coverage --watchAll",
    "build": "cross-env NODE_ENV=production webpack --progress --mode=production",
    "dev": "cross-env NODE_ENV=development webpack --mode=development",
    "startDev": "npm run dev && webpack-dev-server --content-base dev/ --progress --useLocalIp --host 0.0.0.0 --mode=development --open",
    "dev-mock": "cross-env MOCK=true webpack-dev-server --content-base dev/ --progress --useLocalIp --host 0.0.0.0 --mode=development --open",
    "standard": "standard --verbose | snazzy || exit 0",
    "start": "node server.js"
  },
  "keywords": [
    "wiredcraft",
    "test",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "xiaoT",
  "license": "ISC",
  "dependencies": {
    "antd": "^4.2.0",
    "area-data": "^5.0.6",
    "axios": "^0.18.0",
    "history": "^4.7.2",
    "http": "0.0.1-security",
    "koa": "^2.11.0",
    "koa-static": "^5.0.0",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.13.1",
    "react-area-linkage": "^3.0.0",
    "react-dom": "^16.13.1",
    "react-redux": "^5.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^10.4.7",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.13.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "file-loader": "^2.0.0",
    "fsevents": "^2.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "jest": "^25.3.0",
    "mini-css-extract-plugin": "^0.4.2",
    "mocker-api": "^2.0.4",
    "postcss": "^7.0.2",
    "postcss-css-variables": "^0.12.0",
    "postcss-functions": "^3.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "precss": "^3.1.2",
    "react-loadable": "^5.5.0",
    "react-test-renderer": "^16.8.4",
    "standard": "^12.0.1",
    "standard-loader": "^6.0.1",
    "style-loader": "^0.23.0",
    "ts-jest": "^25.3.1",
    "ts-loader": "^7.0.2",
    "url-loader": "^1.1.1",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "engines": {
    "node": "11.14.0",
    "npm": ">= 5.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current",
            "browsers": [
              "Android >= 5.0"
            ]
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import",
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ]
    ]
  },
  "standard": {
    "ignore": [
      "dist/"
    ],
    "global": [
      "JSX",
      "HTMLInputElement",
      "Element",
      "HTMLTextAreaElement"
    ],
    "parser": "babel-eslint"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "^@Components(.*)$": "<rootDir>/src/components$1",
      "^@JS(.*)$": "<rootDir>/src/common/js$1"
    },
    "setupFiles": [
      "./setupJest.js"
    ]
  }
}
